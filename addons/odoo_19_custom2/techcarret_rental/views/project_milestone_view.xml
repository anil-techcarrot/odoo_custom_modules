<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <record id="amount_milestone_project_inherit_view" model="ir.ui.view">
        <field name="name">project.milestone.view.list.inherit</field>
        <field name="model">project.milestone</field>
        <field name="inherit_id" ref="sale_project.project_milestone_view_tree" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='quantity_percentage']" position="before">
                <field name="sale_line_amount" readonly="1" />
                <field name="quantity_amount"  />
            </xpath>
        </field>
    </record>

</odoo>